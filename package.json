{
  "name": "bot",
  "version": "1.0.0",
  "description": "A simple discord bot",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsup src/index.ts --minify",
    "format": "prettier --write \"**/*.{json,ts}\"",
    "lint": "eslint . --ext ts --fix",
    "clean": "npm run lint && npm run format"
  },
  "author": "Jasper Verbruggen",
  "license": "MIT",
  "dependencies": {
    "discord-ytdl-core": "^5.0.4",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "tsup": "^8.0.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
